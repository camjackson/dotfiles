[user]
	name = Cam Jackson
	email = 1930451+camjackson@users.noreply.github.com
[core]
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[log]
    #abbrevCommit = true
    #decorate = true
[format]
    #pretty = oneline
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[pull]
	rebase = false
